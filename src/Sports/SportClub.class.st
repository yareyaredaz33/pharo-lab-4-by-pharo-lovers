Class {
	#name : 'SportClub',
	#superclass : 'Object',
	#instVars : [
		'name',
		'members'
	],
	#category : 'Sports',
	#package : 'Sports'
}

{ #category : 'accessing' }
SportClub >> members [ [
    ^ members.
]
]

{ #category : 'accessing' }
SportClub >> name [ [
    ^ name.
]
]
